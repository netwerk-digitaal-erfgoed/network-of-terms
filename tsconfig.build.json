{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2022",
    "lib": [
      "esnext"
    ],
    "module": "nodenext",
    "declarationMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true
  }
}
